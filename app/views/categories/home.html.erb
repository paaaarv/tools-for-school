<%require 'pry'%>

<h1> Tools For School </h1>


<% @categories.each do |category|%>
<% if category.supplies.length >= 1 %>
  <h4> <%= category.name %> </h4>
    <% see_two(category).each do |supply|%>
      <p> <%= "#{supply.name} | $#{supply.price} | #{supply.amount_to_goal}"%> </p>
    <%end%>
    <%= link_to "See More", category_path(category)%><br>
  <%end%>
<%end%>
